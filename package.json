{
  "name": "with-validation",
  "version": "1.1.0",
  "keywords": [
    "react-js",
    "form-validation",
    "input-validation"
  ],
  "description": "Higher-order ReactJs component that validates a form field.",
  "main": "dist/index.js",
  "repository": "git@github.com:lsiden/with-validation.git",
  "author": "Lawrence Siden <lsiden@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cloc": "^2.2.0",
    "debug": "^3.0.1",
    "enzyme": "^2.9.1",
    "jest": "^21.0.2",
    "react-test-renderer": "^15.6.1",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^.+\\.[s]?css$": "identity-obj-proxy"
    }
  },
  "scripts": {
    "test": "DEBUG=with-validation:* DEBUG_DEPTH=3 NODE_ENV=test jest --verbose",
    "test:watch": "yarn test -- --watch",
    "start": "NODE_ENV=dev webpack-dev-server",
    "build": "NODE_ENV=production babel src/ --ignore __tests__ --out-dir dist/",
    "cloc": "cloc $(git ls-files)"
  }
}
